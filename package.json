{
  "name": "@graphile/sampledb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postgraphile": "postgraphile --watch --disable-default-mutations",
    "load:watch": "nodemon -e sql --watch scripts/schema_pg.sql -x './scripts/load.sh'",
    "start": "npm_config_loglevel=warn concurrently --kill-others 'npm run load:watch' 'sleep 5 && npm run postgraphile'"
  },
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^4.0.0-alpha.0",
    "@graphile/pg-pubsub": "^4.4.0-beta.5",
    "@graphile/subscriptions-lds": "^4.4.0-beta.5",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "postgraphile": "^4.4.0-beta.5",
    "postgraphile-plugin-connection-filter": "^1.0.0-rc.1"
  }
}
